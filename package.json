{
  "name": "graphql-basics",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "heroku-postbuild": "babel src --out-dir dist --copy-files",
    "get-schema": "graphql get-schema -p prisma1",
    "get-schema-2": "graphql-codegen --config ./codegen.yml",
    "dev": "./node_modules/.bin/env-cmd -f ./config/dev.env nodemon  src/index.js --ext js,graphql --exec babel-node",
    "start-local": "./node_modules/.bin/env-cmd -f ./config/prod.env node dist/index.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@graphql-tools/schema": "^8.3.11",
    "@graphql-yoga/node": "^2.2.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "env-cmd": "^10.1.0",
    "graphql": "14.6.0",
    "graphql-cli": "^4.1.0",
    "graphql-import": "^1.0.2",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^8.2.9",
    "graphql-yoga": "^2.3.0",
    "jsonwebtoken": "^8.5.1",
    "prisma-binding": "2.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/schema-ast": "^2.4.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "nodemon": "^2.0.15"
  }
}
